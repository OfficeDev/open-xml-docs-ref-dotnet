{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/toc.yml",
          "**/TOC.yml",
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      },
      {
        "files": [
          "**/TOC.yml",
          "**/toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "openxml_bread_toc",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      },
      {
        "files": [
          "**/TOC.yml",
          "**/toc.yml"
        ],
        "src": "docs/api",
        "dest": "openxml_ref_toc",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      },
      {
        "files": [
          "**/TOC.yml",
          "**/toc.yml"
        ],
        "src": "docs/api/overview/openxml",
        "dest": "openxml_ref_combo_toc",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": {
      "files": [
        "api/overwrites/**/*.md"
      ]
    },
    "globalMetadata": {
      "apiPlatform": "dotnet",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "uhfHeaderId": "MSDocsHeader-Dev_Office",
      "ms.suite": "office365",
      "ms.author": "o365devx",
      "author": "o365devx",
      "ms.topic": "reference",
      "ms.prod": "office",
      "ms.technology": "open-xml",
      "extendBreadcrumb": true
    },
    "fileMetadata": {
      "author": {
        "api/**.yml": "dotnet-bot"
      }
    },
    "template": [],
    "dest": "_site",
    "xref": [
      "_zip/msdn.4.5.2.zip"
    ],
    "lruSize": 0,
    "markdownEngineName": "markdig"
  }
}
