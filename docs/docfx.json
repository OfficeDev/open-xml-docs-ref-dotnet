{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/toc.yml",
          "**/TOC.yml",
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      },
      {
        "files": [
          "**/TOC.yml",
          "**/toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "openxml_bread_toc",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      },
      {
        "files": [
          "**/TOC.yml",
          "**/toc.yml"
        ],
        "src": "api",
        "dest": "openxml_ref_toc",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      },
      {
        "files": [
          "**/TOC.yml",
          "**/toc.yml"
        ],
        "src": "api/overview/openxml",
        "dest": "openxml_ref_combo_toc",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "api/overwrites/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "add/metadata/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif",
          "**/*.bmp"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": {
      "files": [
        "api/overwrites/**/*.md"
      ]
    },
    "globalMetadata": {
      "ms.subservice": "open-xml",
      "ms.service": "office",
      "feedback_system": "Standard",
      "apiPlatform": "dotnet",
      "breadcrumb_path": "/dotnet/open-xml-docs-ref-dotnet-breadcrumb/toc.json",
      "uhfHeaderId": "MSDocsHeader-Dev_Office",
      "ms.suite": "office365",
      "ms.author": "o365devx",
      "author": "o365devx",
      "ms.topic": "managed-reference",
      "products": ["https://authoring-docs-microsoft.poolparty.biz/devrel/6ab06385-661e-4214-8870-bbe4071c960d"],
      "extendBreadcrumb": true
    },
    "fileMetadata": {
      "author": {
        "api/**.yml": "dotnet-bot"
      }
    },
    "template": [],
    "dest": "_site",
    "lruSize": 0,
    "isCanonicalUrlWithMoniker": true,
    "markdownEngineName": "markdig"
  }
}
